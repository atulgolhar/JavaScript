// Conditionals: Switch Statements
// switch ("2") {

switch (3) {
    case 1:
        console.log("Yes 1 matches");
        break;

    case "2":
        console.log('Yes "2" matches');
        break;

    case 3:
        console.log("Yes 3 matches");			// Yes 3 matches
        break;

    default:
        console.log("No match");
}







