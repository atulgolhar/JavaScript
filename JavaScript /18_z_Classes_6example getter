class Pizza {
    constructor() {
        this.kiddo7 = "NOLUMPS";
    }
    getpizzaKidSauce1() {
        return this.kiddo6;
    }
    setpizzaKidSauce2(pizzaKidSauce3) {
        this.kiddo5 = pizzaKidSauce3;
    }
