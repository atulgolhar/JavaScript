// Nullish Coalescing Operator
let dhacSwimClub = prompt("Quinn to swim tonight?");
console.log(dhacSwimClub ?? "You didnt answer the question.");

// note User enters Yes, click Cancel, console shows default answer. ERROR
// note User enters nothing, click OK, console shows blank line. ERROR

